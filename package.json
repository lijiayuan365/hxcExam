{
  "name": "exam",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator 'iPhone 15'",
    "lint": "eslint .",
    "start": "react-native start",
